\def\MakeBlocksES#1#2#3
{
\begin{scope}[xshift=-1 in]

    \foreach\x in {1,...,#1}
    {
        \begin{scope}[shift={(1.1*\x,0)}]
            \HBlockB{(0,0)}
            \end{scope}
            }

            \foreach\x in {1,...,#2}
            {
            \begin{scope}[shift={(.2*\x+#1*1.1+.9,0)}]
            \TBlockB{(0,0)}
            \end{scope}
            }     


            \ifthenelse{#3>5}
            {
                \foreach\x in {1,...,5}{
                    \begin{scope}[shift={(.2*#2+#1*1.1+1.1,.18*\x-.18)}]
                    \OBlockB{(0,0)}
                    \end{scope}
                    }
                \foreach\x in {6,...,#3}{
                    \begin{scope}[shift={(.2*#2+#1*1.1+1.3,.18*\x-.18-.90)}]
                    \OBlockB{(0,0)}
                    \end{scope}
                    }
            }
            {
            \foreach\x in {1,...,#3}
            {
                \begin{scope}[shift={(.2*#2+#1*1.1+1.1,.18*\x-.18)}]
                \OBlockB{(0,0)}
                \end{scope}
            }
    }  
\end{scope}
}


